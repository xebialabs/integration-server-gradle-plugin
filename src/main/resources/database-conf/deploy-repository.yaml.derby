xl.repository:
  artifacts:
    root: build/artifacts
  database:
    db-driver-classname: "org.apache.derby.client.ClientAutoloadedDriver"
    db-url: "jdbc:derby://localhost:{{DB_PORT}}/xldrepo;create=true;user=admin;password=admin"
    db-username: "admin"
    db-password: "admin"
    leak-detection-threshold: 2 minutes
    max-pool-size: 10
