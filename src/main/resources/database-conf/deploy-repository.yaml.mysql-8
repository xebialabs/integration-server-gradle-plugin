xl.repository:
  artifacts:
    root: build/artifacts
  database:
    db-driver-classname: "com.mysql.jdbc.Driver"
    db-url: "jdbc:mysql://localhost:{{DB_PORT}}/xld?autoReconnect=true&useLegacyDatetimeCode=false"
    db-username: "xldrepo"
    db-password: "xldrepo"
    leak-detection-threshold: 2 minutes
    max-pool-size: 10
